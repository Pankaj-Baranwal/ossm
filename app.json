{
  "name": "Convoke.io",
  "description": "Web portal for Convoke 2.0",
  "success_url": "/",
  "addons": [
    "heroku-postgresql:hobby-dev"
  ],
  "env": {
    "SECRET_KEY": {
      "description": "Django SECRET_KEY",
      "generator": "secret",
      "required": true
    },
    "HEROKU_URL": {
      "description": "Public URL to this app",
      "value": "http://convoke-staging.herokuapp.com"
    },
    "PRODUCTION": "true"
  },
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku/python"
    }
  ],
  "repository": "https://github.com/clusterinnovationcentre/ossm"
}
