@require 'frontend'

body.contact
  main.root
    background transparent
    > div
      background $white
      section
        z-index 2

  div.map
    position fixed
    left 0
    bottom 0
    right 0
    width 100vw
    height 100vh
    z-index 0
    iframe
      position relative
      width 100%
      height 100%
      z-index 0
